{
  "name": "recurring-works",
  "version": "1.0.0",
  "description": "Maintenance Preventative Recurring Works Planner/Tracker",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "sequelize db:seed:all",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:concur": "concurrently \"npm run server\" \"npm run client\"",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "server": "node-env-run server --exec nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmawardi/recurring-works.git"
  },
  "keywords": [
    "maintenance"
  ],
  "author": "Dannar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmawardi/recurring-works/issues"
  },
  "homepage": "https://github.com/dmawardi/recurring-works#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "mysql2": "^2.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.2"
  },
  "engines": {
    "npm": "6.13.1",
    "node": "12.1.0"
  },
  "devDependencies": {
    "sequelize-cli": "^5.5.1"
  }
}
